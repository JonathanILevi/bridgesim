{
  "name": "bridgesim",
  "version": "1.0.0",
  "description": "It's a starship bridge simulator.",
  "main": "index.js",
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/mocha": "^2.2.33",
    "@types/sinon": "^1.16.31",
    "@types/webrtc": "0.0.21",
    "bower": "^1.7.9",
    "browserify": "^13.0.1",
    "chai": "^3.5.0",
    "crisper": "^2.0.2",
    "html-minifier": "^2.1.2",
    "http-server": "^0.9.0",
    "mocha": "^3.1.2",
    "sinon": "^1.17.4",
    "typescript": "^2.0.7",
    "vulcanize": "^1.14.8",
    "watchify": "^3.7.0"
  },
  "scripts": {
    "test": "tsc && mocha $(find . -name '*_test.js' ! -path './node_modules/*' ! -path './bower_components/*'| tr -s '\n' ' ')",
    "watch": "watchify $(find client/ -name '*.ts' ! -name '*_test.ts' | tr -s '\n' ' ') -p tsify --outfile out.js --debug --verbose",
    "watch-test": "tsc --watch & mocha $(find . -name '*_test.js' ! -path './node_modules/*' ! -path './bower_components/*'| tr -s '\n' ' ') --watch",
    "clean": "rm -rf build && find . -name '*.js' ! -path './util/build.js' ! -path './node_modules/*' ! -path './bower_components/*' | xargs rm",
    "build": "node ./util/build.js",
    "init": "bower install",
    "serve": "http-server -c-1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aomarks/bridgesim.git"
  },
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/aomarks/bridgesim/issues"
  },
  "homepage": "https://github.com/aomarks/bridgesim#readme",
  "dependencies": {
    "brace-expansion": "^1.1.6",
    "doctrine": "^2.0.0",
    "minifyify": "^7.3.3",
    "source-map-concat": "^1.0.1",
    "source-map-dummy": "^1.0.0",
    "source-map-resolve": "^0.5.0",
    "svgo": "^0.6.6",
    "tsify": "^1.0.3",
    "typescript-collections": "^1.1.2",
    "uglify-js": "^2.7.0"
  },
  "resolutions": {
    "convert-source-map": "1.3.0",
    "mkdirp": "0.5.1",
    "source-map": "0.5.6",
    "inherits": "2.0.3",
    "readable-stream": "2.1.5",
    "isarray": "1.0.0",
    "minimist": "1.2.0",
    "minimatch": "3.0.3",
    "colors": "1.1.2",
    "escape-string-regexp": "1.0.5",
    "supports-color": "2.0.0",
    "strip-json-comments": "2.0.1",
    "glob": "7.1.1",
    "once": "1.4.0",
    "wordwrap": "1.0.0",
    "punycode": "1.4.1",
    "acorn": "4.0.3",
    "type-detect": "1.0.0",
